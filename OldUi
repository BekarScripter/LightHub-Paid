-- Anti Kick --
Main:NewButton("Anti Kick", "Prevents you from getting kicked.", function()
	local Path = game.Players.LocalPlayer
	local hook;
	hook = hookmetamethod(game, "__namecall", newcclosure(function(...)
		local Args = {...}
		local Self = Args[1]
		local method = getnamecallmethod()
		if not checkcaller() and Self == Path and method == "Kick" then
			return
		end
		return hook(...)
		end))
end)
Main:NewButton("Max Cash", "Gives you Max Cash.", function()
