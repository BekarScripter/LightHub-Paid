--> Misc <--
    local Misc = Window:NewTab("Misc")
	local Misc = Misc:NewSection("Misc")
	-- Information --
	Misc:NewButton("Auto-Heal", "Auto-Heals if health is below 70", function()
		getgenv().AutoHeal = true

        local chr = game:GetService'Players'.LocalPlayer.Character
        local bp = game:GetService'Players'.LocalPlayer.Backpack
        local hum = chr:FindFirstChild("Humanoid")
        local isTC = false
        
        local function buy()
            local args = {
            [1] = "Health Booster",
            [2] = "Single",
            [3] = Color3.new(0, 0, 0)
        }
        
        game:GetService("ReplicatedStorage")["_CS.Events"].PurchaseTeamItem:FireServer(unpack(args))
        wait(2)
        end
        
        local function eq()
            bp:FindFirstChild("Health Booster").Parent = chr    
        end
        
        local function tc()
            if chr.Head:FindFirstChild("PlayerDisplay").TeamName.Text == "[Paramedic]" then 
                isTC = true
                warn("Is in Paramedic team")
                else
                warn("You are not in the Paramedic team")
            end
        end
        
        local function use()
        local args = {
            [1] = "HealthBooster"
        }
        
        game:GetService("ReplicatedStorage")["_CS.Events"].ToolEvent:FireServer(unpack(args))
        end
        
        
        local function check()
            if isTC == true then
            if hum.Health <= 70 then
                warn("Health is lower than 70... Trying to heal")
                buy()
                wait(.1)
                eq()
                wait(.1)
                use()
                else
                warn("Health is higher than 70")
            end
        end
        end
        
        
        while wait() do
           if getgenv().AutoHeal == true then
                tc()
                wait(.1)
                check()
        
            end
        end
	end)

-- 50k Reset --
Misc:NewButton("50K reset", "Resets your money back to 50K.", function()
	for i,v in pairs(workspace.PlayerVehicles:GetChildren()) do
        game:GetService("ReplicatedStorage")["_CS.Events"].FillUpCar:FireServer(v, 9e9)
    end
    wait(.2)
    game:GetService("TeleportService"):Teleport(game.PlaceId)
end)

-- Bypass -- --// WORKING!! \\ --
Misc:NewButton("Bypass Anti Cheat", "Bypasses their Anti-Cheat", function()
	repeat wait() until game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored == false
    local time = tick()
    for i, v in next, getconnections(game:GetService("Players").LocalPlayer.Character.DescendantAdded) do
        v:Disable()
	end
    local success, err = pcall(function()
        local mt = getrawmetatable(game)
        setreadonly(mt, false)
        local namecall = mt.__namecall

        mt.__namecall = function(self,...)
            local args = {...}

            if tostring(getnamecallmethod()) == 'FindPartOnRayWithWhitelist' and getcallingscript() == game.Players.LocalPlayer.PlayerGui['_L.Handler'].GunHandlerLocal then
                wait(9e9)
                return
            end
            return namecall(self,...)
        end
    end)
    
if success then
    
print("done")
    
else
    
print("loser")
    
end
end)

