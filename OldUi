			end)
--> RemoveStuff <--
	local RemoveStuff = Window:NewTab("Remove Stuff")
	local RemoveStuff = RemoveStuff:NewSection("Remove Face & Tattoos")
	-- Remove Blur --
	RemoveStuff:NewButton("Remove Blur", "Removes the Blur effect", function()
				while wait() do
					for fd, fe in pairs(game:GetService("Workspace").Camera:GetChildren()) do
					  fe:Destroy()
					end
			  end
			  if game:GetService("Players").LocalPlayer.PlayerGui.Dmg then
				game:GetService("Players").LocalPlayer.PlayerGui.Dmg:Destroy()
				end
			end)
	-- Remove Ragdoll --
	RemoveStuff:NewButton("Remove Ragdoll", "Removes your Ragdoll", function()
			game:GetService("RunService").Stepped:Connect(function()
				if game:GetService("Players").LocalPlayer.Character.Head.EDead then
					game:GetService("Players").LocalPlayer.Character.Head.EDead:Destroy()
		game.Players.LocalPlayer.Character:FindFirstChild("Ragdoller"):Destroy()
		game.Players.LocalPlayer.Character:FindFirstChild("HurtSystem"):Destroy()
		end
		end)
		end)
	-- Remove Face & Tattoos --
	RemoveStuff:NewButton("Remove", "Removes your Face & Tattoos", function()
		for i,v in pairs(game.Players.LocalPlayer.Character.Head:GetChildren()) do
			if v:IsA("Decal") then
				v:Destroy()
			end
		end
	end)
	-- Removes Hair / Bald --
	RemoveStuff:NewButton("Bald", "Removes your Hair", function()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Accessory") then
				v:Destroy()
				print("Done")
			end
		end
	end)
	-- Remove Pants --
	RemoveStuff:NewButton("Remove Pants", "Removes your Pants", function()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Pants") then
				v:Destroy()
				print("Done")
			end
		end
	end)
	-- Remove Shirt --
	RemoveStuff:NewButton("Remove Shirt", "Removes your Shirt", function()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Shirt") then
				v:Destroy()
				print("Done")
			end
		end
	end)
	-- Remove All --
	RemoveStuff:NewButton("Remove All", "Removes your All Removeables", function()
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Shirt") then
				v:Destroy()
				print("Done")
			end
		end
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Pants") then
				v:Destroy()
				print("Done")
			end
		end
		for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Accessory") then
				v:Destroy()
				print("Done")
			end
		end
		for i,v in pairs(game.Players.LocalPlayer.Character.Head:GetChildren()) do
			if v:IsA("Decal") then
				v:Destroy()
			end
		end
		game.Players.LocalPlayer.Character.Head.Gui:Remove()
	end)
--> Exploits <--
    local UsefulExploits = Window:NewTab("Useful Exploits")
	local UsefulExploits = UsefulExploits:NewSection("Useful Exploits")
	-- Cbring --
	UsefulExploits:NewTextBox("Cbring", "Brings the player, target won't see it and killable :).", function(txt)
		 for i,v in pairs(game.Players:GetChildren()) do
			if (string.sub(string.lower(v.Name),1,string.len(txt))) == string.lower(txt) then
			txt = v.Name
			end
		 end
		 local name = txt
		 function cbringTarget()
			repeat
				task.wait()
			 game.Players[name].Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position + game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 3.6)
			 until game.Players[name].Character.Humanoid.Health == 0
			end
			
		 cbringTarget()
			
			end)
	-- View Player --
	UsefulExploits:NewTextBox("View Player", "Makes you view the targeted player.", function(txt)
		 for i,v in pairs(game.Players:GetChildren()) do
		 if (string.sub(string.lower(v.Name),1,string.len(txt))) == string.lower(txt) then
		 txt = v.Name
		 end
		 end
		 game.Workspace.CurrentCamera.CameraSubject = game.Players[txt].Character
		 task.wait(15)
		 game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
			end)
	
--> Extras <--
    local Extras = Window:NewTab("Extras")
	local Extras = Extras:NewSection("Extras")
	-- Rejoin --
	Extras:NewButton("Rejoin", "Rejoins the game for you.", function()
			local teles = game.TeleportService
			local p = game.Players.LocalPlayer
			teles:Teleport(game.PlaceId, p)
		end)
	-- Pickup Tools --
	Extras:NewButton("Pickup Tools", "Makes you pickup all dropped tools.", function()
		 local g = game.Workspace.tools
		 print("Soon a toggle for it!")
		 for fk, fl in pairs((g:GetChildren())) do
			if fl:IsA("Tool") and fl.Name == "Fist" and fl.Name ~= "Phone" and fl.Name ~= "Crate" then
				game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(fl)
				break
			end
		 end
			end)
	-- Fov --
	Extras:NewTextBox("Custom Fov", "Type your custom Fov in here.", function(txt)
				while task.wait() do
		 game.Workspace.CurrentCamera.FieldOfView = txt
				end
		end)
	-- Anti Afk --
	Extras:NewButton("Anti Afk", "Makes roblox think you're not afk", function()
				local vu = game:GetService("VirtualUser")
				game:GetService("Players").LocalPlayer.Idled:connect(function()
				   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
				   wait(1)
				   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			   end)
			end)

