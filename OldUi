Main:NewButton("Infinite Health (Im Fr)", "Shitty ass game", function()
	local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(a, b)
    if tostring(a) == "Humanoid" then
        if tostring(b) == "Health" then
            return 9e9;
        end
    end
    return old_index(a, b);
end
local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(a, b)
    if tostring(a) == "Humanoid" then
        if tostring(b) == "MaxHealth" then
            return 9e9;
        end
    end
    return old_index(a, b);
end
end)

Main:NewButton("Infinite Stamina", "Gives you Max Stamina", function()
	local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(a, b)
    if tostring(a) == "MaxStamina" then
        if tostring(b) == "Value" then
            return 9e9;
        end
    end
    return old_index(a, b);
end
local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(a, b)
    if tostring(a) == "Stamina" then
        if tostring(b) == "Value" then
            return 9e9;
        end
    end
    return old_index(a, b);
end

local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(a, b)
    if tostring(a) == "Humanoid" then
        if tostring(b) == "WalkSpeed" then
            return 16;
        end
    end
    return old_index(a, b);
end
end)
end
