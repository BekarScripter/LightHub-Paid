        w.MaxAmmo = math.huge;
        w.CurrentAmmo = math.huge;
        w.StoredAmmo = math.huge;
        w.FireRate = 0;
        w.AutoFire = true;
        w.Range = 1000;
        w.Spread = 0;
        w.ReloadTime = 0;
        w.Bullets = 1;
	end)

